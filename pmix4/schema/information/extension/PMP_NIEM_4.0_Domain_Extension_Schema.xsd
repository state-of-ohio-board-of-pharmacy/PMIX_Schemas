			<xsd:documentation>Information from a completed prescription form and additional information add through PMP analysis.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="pmp:Dispenser" minOccurs="0" maxOccurs="unbounded"/>
