	<!--Properties of Type pmp:PrescriberType-->
	<xsd:element name="Prescriber" type="pmp:PrescriberType" nillable="true"/>
	<xsd:element name="RequestPrescriber" type="pmp:PrescriberType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The PMP state that receives the request from outside the state jurisdiction of the initiator and will execute the request.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
