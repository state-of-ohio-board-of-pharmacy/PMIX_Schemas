	<xsd:complexType name="CompoundDrugIngredientType">
		<xsd:annotation>
			<xsd:documentation>Ingredients that make up a compound drug.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="pmp:CompoundDrugIngredientSequenceNumber" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:CompoundDrugIngredientProductIdentifier" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:CompoundDrugIngredientProductNameText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:CompoundDrugIngredientStrengthText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:CompoundDrugIngredientDEAClassScheduleText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:IngredientQuantity" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:CompoundDrugIngredientDrugDosageUnitsCode" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PatientAlertType">
		<xsd:annotation>
			<xsd:documentation>Ingredients that make up a compound drug.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="pmp:PatientAlertDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PatientAlertCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PatientAlert" type="pmp:PatientAlertType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				An alert about a patient.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PatientAlertDescriptionText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				A textual description of an alert about a patient.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PatientAlertCategoryCode" type="pmpcd:PatientAlertCategoryCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				A code for a kind of an alert about a patient.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
