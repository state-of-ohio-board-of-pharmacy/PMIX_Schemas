<xsd:schema
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/"
	xmlns:j="http://release.niem.gov/niem/domains/jxdm/6.2/"
	xmlns:niem-xsd="http://release.niem.gov/niem/proxy/xsd/4.0/"
	xmlns:i="http://release.niem.gov/niem/appinfo/4.0/"
	xmlns:pmp="http://pmixpmp.org/niem/4.0/extension"
	xmlns:s="http://release.niem.gov/niem/structures/4.0/"
	xmlns:pmpcd="http://pmixpmp.org/pmpcode"
	targetNamespace="http://pmixpmp.org/niem/4.0/extension"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<xsd:annotation>
		<xsd:documentation>The NIEM 4.0 PMP domain specific extension schema.</xsd:documentation>
	</xsd:annotation>
	<!--
  Subset schemas
  -->
	<xsd:import namespace="http://release.niem.gov/niem/structures/4.0/" schemaLocation="../subset/niem/utility/structures/4.0/structures.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/appinfo/4.0/" schemaLocation="../subset/niem/utility/appinfo/4.0/appinfo.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/4.0/" schemaLocation="../subset/niem/proxy/xsd/4.0/xs.xsd"/>
	<xsd:import namespace="http://pmixpmp.org/pmpcode" schemaLocation="../code_set/PMIX_Code_Set.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/4.0/" schemaLocation="../subset/niem/niem-core/4.0/niem-core.xsd"/>
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/6.2/" schemaLocation="../subset/niem/domains/jxdm/6.2/jxdm.xsd"/>
	<!--
  Declarations of abstract properties used in PMP schema - in Alphabetical Order
  -->
	<xsd:element name="CompoundDrugIngredientProductIdentifier" abstract="true">
		<xsd:annotation>
			<xsd:documentation>
        This item must be substituted with pmp:CompoundDrugIngredientDINProductIdentifier, pmp:CompoundDrugIngredientHRIProductIdentifier,
        pmp:CompoundDrugIngredientNDCProductIdentifier, pmp:CompoundDrugIngredientUPCProductIdentifider or pmp:CompoundDrugIngredientUPNProductIdentifier
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugProductIdentifier" abstract="true">
		<xsd:annotation>
			<xsd:documentation>
        This property must be substituted with pmp:DrugDINProductIdentifier, pmp:DrugHRIProductIdentifier, pmp:DrugNDCProductIdentifier,
        pmp:DrupUPCProductIdentifier or pmp:DrugUPNProductIdentifier
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="PersonIdentifier" abstract="true">
        <xsd:annotation>
            <xsd:documentation>
                This property must be substituted by one of the following:
                pmp:PersonDriverLicenseIdentifier,
                pmp:PersonMilitaryIdentifier,
                pmp:PersonOtherIdentifier,
                pmp:PersonPassportIdentifier,
                pmp:PersonSocialSecurityNumberIdentifier,
                pmp:PersonStateIssuedIdentifier,
                pmp:PersonTribalIdentifier,
                pmp:PersonUniqueSystemIdentifier,
                pmp:PersonPermanentResidentCardIdentifier,
                pmp:PersonVeterinaryPatientMicrochipIdentifier,
                pmp:PersonMedicaidRecipientIdentifier
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
	<xsd:element name="PrescriptionReport" abstract="true">
		<xsd:annotation>
			<xsd:documentation>
        This property must be substituted with pmp:PharmacyReportingPrescriptionReport or pmp:RequestResponsePrescriptionReport
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--
  Substituation properties used in PMP schema - in Alphabetical Order
  -->
	<xsd:element name="CompoundDrugIngredientDINProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:CompoundDrugIngredientProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies an ingredient in a compound drug using the DIN
        Replaces CDI02 and CDI03 when CDI02 is 05 DIN
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientHRIProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:CompoundDrugIngredientProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies an ingredient in a compound drug using the HRI
        Replaces CDI02 and CDI03 when CDI02 is 03 HRI
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientNDCProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:CompoundDrugIngredientProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies an ingredient in a compound drug using the NDC
        Replaces CDI02 and CDI03 when CDI02 is 01 NDC
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientUPCProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:CompoundDrugIngredientProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies an ingredient in a compound drug using the UPC
        Replaces CDI02 and CDI03 when CDI02 is 02 UPC
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientUPNProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:CompoundDrugIngredientProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies an ingredient in a compound drug using the UPN
        Replaces CDI02 and CDI03 when CDI02 is 04 UPN
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugCPDProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Used to indicate that the drug is a compound drug.  Use the pmp:CompoundDrug to transmit the ingredients in the drug
        Replaces DSP07 and DSP08 when DSP07 is 06 Compound
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugDINProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies a drug using the DIN
        Replaces DSP07 and DSP08 when DSP07 is 05 DIN
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugHRIProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies a drug using the HRI
        Replaces DSP07 and DSP08 when DSP07 is 03 HRI
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugNDCProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies a drug using the NDC
        Replaces DSP07 and DSP08 when DSP07 is 01 NDC
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugUPCProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies a drug using the UPC
        Replaces DSP07 and DSP08 when DSP07 is 02 UPC
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugUPNProductIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:DrugProductIdentifier">
		<xsd:annotation>
			<xsd:documentation>
        Identifies a drug using the UPN
        Replaces DSP07 and DSP08 when DSP07 is 04 UPN
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="PersonPermanentResidentCardIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their permanent resident card (green card).
                This property and its subproperties replace
                PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03, AIR04 and AIR05.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonVeterinaryPatientMicrochipIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a pet using their microchip number
                This property and its subproperties replace
                PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03, AIR04 and AIR05.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonMedicaidRecipientIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their medicaid recipient identifier.
                This property and its subproperties replace
                PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03, AIR04 and AIR05.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonDriverLicenseIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their driver's license number and the entity which has jurisdiction over the
                licensee based on the license.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonMilitaryIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their military id number and the entity which has jurisdiction over the
                licensee based on the license.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonNationalHealthIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their national health ID number and the entity which has jurisdiction over the
                ID.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonOtherIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using an id number and the entity which has jurisdiction over the licensee based on
                the license.
                This property and its subproperties replace
                PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03, AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonPassportIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their passport number and the entity which has jurisdiction over the passport.
                This property and its subproperties replace
                PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03, AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonSocialSecurityNumberIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their social security number and the entity which has jurisdiction over the
                number.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonStateIssuedIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their state identification number and the entity which has jurisdiction over
                the identification.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonTribalIdentifier" type="nc:IdentificationType" substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using their tribal id number and the entity which has jurisdiction over the id.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonUniqueSystemIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using a unique system number and the entity which has jurisdiction over the number.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="PersonVendorSystemIdentifier" type="nc:IdentificationType"
                 substitutionGroup="pmp:PersonIdentifier">
        <xsd:annotation>
            <xsd:documentation>
                Identifies a person using an ID number provided by a vendor and the vendor which has jurisdiction over
                the number.
                This property and its subproperties replace PAT01, PAT02 and PAT03 or PAT04, PAT05 and PAT06 or AIR03,
                AIR04 and AIR05
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
	<xsd:element name="PharmacyReportingPrescriptionReport" type="pmp:PharmacyReportingPrescriptionReportType" substitutionGroup="pmp:PrescriptionReport">
		<xsd:annotation>
			<xsd:documentation>
        A PharmacyReportingPrescriptionReport is substituted for a PrescriptionReport when the transmitting entity is a dispenser reporting prescription
        information to a PMP. This was designed as a potential alternative to the x12 version of ASAP 4.1.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportDateRangeBegin" type="niem-xsd:date" substitutionGroup="nc:DateRepresentation" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identifies the beginning of the data range of prescriptions included in the report.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportDateRangeEnd" type="niem-xsd:date" substitutionGroup="nc:DateRepresentation" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identifies the end of the date range of prescriptions included in the report.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RequestPrescriptionDateRangeBegin" type="niem-xsd:date" substitutionGroup="nc:DateRepresentation" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Used in a request for prescription data to indicate the beginning of the date range in which the responding PMP should search
        for prescriptions matching the search criteria.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RequestPrescriptionDateRangeEnd" type="niem-xsd:date" substitutionGroup="nc:DateRepresentation" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Used in a request for prescription data to indicate the beginning of the date range in which the responding PMP should search
        for prescriptions matching the search criteria.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RequestResponsePrescriptionReport" type="pmp:RequestResponsePrescriptionReportType" substitutionGroup="pmp:PrescriptionReport">
		<xsd:annotation>
			<xsd:documentation>
        A prescription report sent to a PMP from another PMP in response to a request for information by that PMP.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--
  Delcarations of properties of types defined in subset schemas - in Alphabetical Order
  -->
	<xsd:element name="ChainSiteIDText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identification number of a particular store within a chain PHA12
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientDEAClassScheduleText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The Class Identification assigned to the ingredient by DEA
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientDrugDosageUnitsCode" type="pmpcd:DrugDosageUnitsCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identifies the unit of measure for the quantity dispensed.
        Replaces CDI05
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientProductNameText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Common name of the compound drug ingredient
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientSequenceNumber" type="niem-xsd:nonNegativeInteger" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The first reportable ingredient is 1. Each additional reortable ingredient is incremented by 1. CDI01
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CompoundDrugIngredientStrengthText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The strength of the compound drug ingredient
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ContactNameText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Name of the person to be contacted regarding questions about this report.
        Replaces PHA11
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DaysSupplyCount" type="niem-xsd:nonNegativeInteger" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The number of days the prescription will last when taken as directed. DSP10
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DEAClassScheduleText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The Class Identification assigned to the drug by DEA
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DEANumberIdentifier" type="pmp:DEANumberIdentifierType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				Drug Enforcement Agency unique identification number. Required when the prescription is for a DEA-defined controlled substance.
        Replaces PHA03 and PRE02 (PRE09 with XDEA flag)
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DispensedQuantity" type="niem-xsd:decimal" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Numeric quantity of a drug dispensed to the patient DSP09
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
        <xsd:element name="RemainingQuantity" type="xsd:nonNegativeInteger" nillable="true">
                <xsd:annotation>
                        <xsd:documentation>
                                Total quantity remaining for prescription.
                                DSP29
                        </xsd:documentation>
                </xsd:annotation>
        </xsd:element>
	<xsd:element name="DrugDosageUnitsCode" type="pmpcd:DrugDosageUnitsCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identifies the unit of measure for the quantity dispensed.
        Replaces DSP11
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugProductNameText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Product Name of the Drug.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugRefillNumberCount" type="niem-xsd:nonNegativeInteger" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Number of the fill of the prescription. 0 indicates original dispensing. DSP06
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugStrengthText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The level of potency of the drug.
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DrugUnitOfMeasureText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
					The unit in which the particular drug is measured
				</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DocumentPublicationTime" type="niem-xsd:time" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Time the report was generated for publication.
        Replaces TH06
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FileTypeCode" type="pmpcd:FileTypeCodeType">
		<xsd:annotation>
			<xsd:documentation>
        Code specifying the type of transaction
        Replaces TH07
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ICD-10DiagnosticCodeText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				ICD-10 code for a prescription. DSP25
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InformationSourceEntityNameText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Entity Name of the Information Source
        Replaces IS02
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IngredientQuantity" type="niem-xsd:decimal" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Amount of the ingredient included in the compound
        Replaces CDI04
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MessageText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Freeform text message.
        Replaces IS03
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MethodOfPaymentCode" type="pmpcd:MethodOfPaymentCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Indicates the primary payment source provided for the prescription
        Replaces DSP16
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NCPDPIdentifier" type="nc:IdentificationType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identifier assigned by the National Council of Prescription Drug Programs. PHA02
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="NPIIdentifier" type="nc:IdentificationType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        National Provider Identifier DSP14 PRE01 PHA01
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OpioidTreatmentCategoryCode" type="pmpcd:OpioidTreatmentCategoryCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				The reason for anÂ opioid prescription...
				Replaces DSP24
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Organization" type="nc:OrganizationType"/>
	<xsd:element name="PartialFillIndicator" type="niem-xsd:boolean" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        To indicate whenter the quantity dispensed is a partial fill.
        Replaces DSP13
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PatientLocationCode" type="pmpcd:PatientLocationCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Indicates where the patient is located when receiving pharmacy services
        Replaces PAT21
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PatientNameOfAnimalText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Name of animal for veterinary medications
        Replaces PAT23
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
        <xsd:element name="PatientPreferredOrAliasLastName" type="nc:TextType" nillable="true">
               <xsd:annotation>
                       <xsd:documentation>
                               ASAP 5 field PAT24
                       </xsd:documentation>
               </xsd:annotation>
       </xsd:element>
       <xsd:element name="PatientPreferredOrAliasFirstName" type="nc:TextType" nillable="true">
               <xsd:annotation>
                       <xsd:documentation>
                               ASAP 5 field PAT25
                       </xsd:documentation>
               </xsd:annotation>
       </xsd:element>
	<xsd:element name="Person" type="nc:PersonType"/>
	<xsd:element name="PersonPrimaryContactInformation" type="nc:ContactInformationType">
		<xsd:annotation>
			<xsd:documentation>
        Holds telephone number and address of a person
        Replaces PAT12, PAT13, PAT14, PAT15, PAT16, PAT17, PRE08
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescribedQuantity" type="niem-xsd:decimal" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				Numeric quantity of a drug prescribed to the patient DSP22
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescriberDEANumberSuffixText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Suffix assigned to a prescriber by an institution for identification when the prescriber is authorized to prescribe
        using the instituation's DEA number
        Replaces PRE03
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescriptionFilledDate" type="nc:DateType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Date the prescription was filled
        replaces DSP05
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescriptionNormCodeText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Used for electronic prescriptions to capture the prescribed drug product identification
        Replaces DSP19
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescriptionNormProductQualifierCode" type="pmpcd:PrescriptionNormProductQualifierCodeType">
		<xsd:annotation>
			<xsd:documentation>
				Used for electronic prescriptions to capture the prescribed drug product identification
				Replaces DSP19
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PrescriptionNumberText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Serial number assigned to the prescription by the pharmacy.
        Replaces DSP02
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="PrescriptionSoldDate" type="nc:DateType" nillable="true">
        <xsd:annotation>
            <xsd:documentation>
                This field is used to determine the date the prescription was dispensed (sold to, picked up by, or
                otherwise left the pharmacy), not the date it was prepared.
                This date could be captured from the point-of-sale (POS) system, if the pharmacy has a POS system,
                and there is a bidirectional flow with the pharmacy management system in order to capture and report
                this date.
                Or it could be captured and reported from a will-call management system, integrated
                with the pharmacy management system.

                Replaces DSP17
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TimeWritten" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
            <xsd:documentation>
                A precise time a prescription was written.
                DSP26
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TimeFilled" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
            <xsd:documentation>
                A precise time a prescription was filled.
                DSP27
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TimeSold" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
            <xsd:documentation>
                A precise time a prescription was sold.
                DSP28
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
	<xsd:element name="PrescriptionWrittenDate" type="nc:DateType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Date the prescription was written by the prescriber
        Replaces DSP03
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RefillsAuthorizedCount" type="niem-xsd:nonNegativeInteger" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Number of times the prescriber has authorized the prescription to be refilled.
        Replaces DSP04
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RelationshipToPatientCode" type="pmpcd:RelationshipToPatientCodeType">
		<xsd:annotation>
			<xsd:documentation>
        Indicates an agent's relationship to a patient
        Replaces AIR06
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportExecutionDate" type="niem-xsd:date" substitutionGroup="nc:DateRepresentation" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The Date the report was generated. Replaces TH05
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportExecutionTime" type="niem-xsd:time" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        The Time the report was generated. Replaces TH06
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ReportingStatusCode" type="pmpcd:ReportingStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>
        Indicates whether the information is new, has changed or should be removed from the PMP's database
        Replaces DSP01
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ResponseIDText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Contains the Transaction Control Number of a transaction that initiated the transaction - used in a response transaction only.
        Replaces TH04
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="RoutingNumber" type="niem-xsd:nonNegativeInteger" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Identification of a specific PMP that the transaction should be routed to
        Replaces TH08
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SpeciesCode" type="pmpcd:SpeciesCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Indicates whether the patient is human or an animal.
        Replaces PAT20
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateIssuedRxSerialNumberIdentification" type="nc:IdentificationType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Number assigned to a state issued serialized prescription blank as well as the state which issued it.
        Replaces AIR01 and AIR02
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="StateLicenseIdentifier" type="nc:IdentificationType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
				A license that gives the organization the right to prescribe drugs for patients. This field can be used to denote prescriptions from Veterinarians. DSP15 PRE04 PHA13
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransactionControlNumberText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Sender assigned code uniquely identifying a transaction.
        Replaces TH02
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransactionTypeCode" type="pmpcd:TransactionTypeCodeType">
		<xsd:annotation>
			<xsd:documentation>
        Identifies the purpose of initiating the transaction.
        Repalces TH03
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TransmissionFormOfRxOriginCode" type="pmpcd:TransmissionFormOfRxOriginCodeType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Code indicating how the pharmacy received the prescription
        Replaces DSP12
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="UniqueInformationSourceIDText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        Reference number or identification number as defined by the business partners.
        Replaces IS01
      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="VersionReleaseNumberText" type="nc:TextType" nillable="true">
		<xsd:annotation>
			<xsd:documentation>
        		Replaces TH01
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="XDEAIndicator" type="niem-xsd:boolean" nillable="true">
	<xsd:annotation>
		<xsd:documentation>
			True is a DEA number is an XDEA number; false otherwise.
		</xsd:documentation>
	</xsd:annotation>
	</xsd:element>
	<!--
  Types defined in the PMP Extension Schema and their associated declarations
