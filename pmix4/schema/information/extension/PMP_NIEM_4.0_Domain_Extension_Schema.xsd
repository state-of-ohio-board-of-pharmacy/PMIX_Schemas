					<xsd:element ref="pmp:DrugRefillNumberCount" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescriptionFilledDate" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:Patient" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:Prescriber" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescriptionWrittenDate" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescriptionDrug" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:RefillsAuthorizedCount" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:DispensedQuantity" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:DaysSupplyCount" minOccurs="1" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:DrugDosageUnitsCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:MethodOfPaymentCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:TransmissionFormOfRxOriginCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PartialFillIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:Pharmacist" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:StateIssuedRxSerialNumberIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescribedQuantity" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:ICD-10DiagnosticCodeText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:OpioidTreatmentCategoryCode" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescriptionSoldDate" minOccurs="0" maxOccurs="unbounded"/>
                                        <xsd:element ref="pmp:RemainingQuantity" minOccurs="1" maxOccurs="1" />
                                        <xsd:element ref="pmp:TimeWritten" minOccurs="0" maxOccurs="1" />
                                        <xsd:element ref="pmp:TimeFilled" minOccurs="0" maxOccurs="1" />
                                        <xsd:element ref="pmp:TimeSold" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
