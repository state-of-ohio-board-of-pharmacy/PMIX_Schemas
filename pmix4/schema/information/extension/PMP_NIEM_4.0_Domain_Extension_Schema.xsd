			<xsd:extension base="nc:PersonType">
				<xsd:sequence>
					<xsd:element ref="pmp:PersonPrimaryContactInformation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:NPIIdentifier" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:DEANumberIdentifier" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:PrescriberDEANumberSuffixText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="pmp:StateLicenseIdentifier" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
